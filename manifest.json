{
  "manifest_version": 3,
  "name": "Title and URL Copier",
  "version": "1.0.0",
  "description": "現在のウェブページのタイトルとURLをクリップボードにコピーします。",
  "permissions": ["activeTab","scripting","clipboardWrite"],
  "background": { "service_worker": "background.js" },
  "action": { "default_title": "タイトルとURLをコピー" }
}
